{
    "presets":[
        "@babel/react",
        "@babel/flow",
        "@babel/preset-env"
    ],
    "plugins": [
        "@babel/syntax-dynamic-import",
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread"
    ],
    "env": {
        "test": {
            "presets":[
                [   "@babel/preset-env",
                    {
                        "modules": "commonjs"
                    }
                ]
            ],
            "plugins": [
                "@babel/plugin-proposal-class-properties"
            ]
        }
    }
}
