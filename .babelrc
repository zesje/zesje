{
    "presets":[
        "@babel/react",
        "@babel/flow"
    ],
    "plugins": [
        "@babel/syntax-dynamic-import",
        "react-hot-loader/babel"
    ],
    "env": {
        "test": {
            "presets":[
                [   "env",
                    {
                        "modules": "commonjs"
                    }
                ]
            ],
            "plugins": [
                "transform-class-properties"
            ]
        }
    }
}
