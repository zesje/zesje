{
  "name": "zesje",
  "main": "index.js",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "concurrently --kill-others --names \"WEBPACK,MYSQL,PYTHON,CELERY,REDIS,OAUTH2\" --prefix-colors \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgRed.bold,bgCyan.bold\" \"yarn dev:webpack\" \"yarn dev:mysql start -i --allow-running\" \"yarn start\" \"yarn dev:celery\" \"yarn dev:redis\" \"yarn dev:oauth\"",
    "dev:backend": "concurrently --kill-others --names \"MYSQL,PYTHON,CELERY,REDIS,OAUTH2\" --prefix-colors \"bgMagenta.bold,bgGreen.bold,bgRed.bold,bgYellow.bold,bgCyan.bold\" \"yarn dev:mysql start -i --allow-running\" \"yarn start\" \"yarn dev:celery\" \"yarn dev:redis\" \"yarn dev:oauth\"",
    "dev:webpack": "webpack-dev-server --hot --inline --progress --config webpack.dev.js",
    "dev:celery": "ZESJE_SETTINGS=$(pwd)/zesje_dev_cfg.py celery -A zesje.wsgi.celery worker -l info --autoscale=4,1 --max-tasks-per-child=16",
    "dev:redis": "redis-server redis.conf",
    "build": "webpack --config webpack.prod.js",
    "ci": "yarn lint; yarn test",
    "lint": "yarn lint:js; yarn lint:py",
    "test": "yarn test:js; yarn test:py",
    "lint:js": "standard --verbose | snazzy",
    "test:js": "jest",
    "lint:py": "flake8",
    "comwemand:test:py": "ZESJE_SETTINGS=$(pwd)/zesje_test_cfg.py python3 -m pytest -v -W error::RuntimeWarning",
    "test:py": "yarn dev:mysql is-running && (yarn command:test:py || true) || ((yarn dev:mysql start && yarn command:test:py); yarn dev:mysql stop)",
    "start": "ZESJE_SETTINGS=$(pwd)/zesje_dev_cfg.py python3 zesje",
    "analyze": "webpack --config webpack.prod.js --profile --json > stats.json; webpack-bundle-analyzer stats.json zesje/static",
    "migrate": "FLASK_APP=zesje.wsgi:app flask db upgrade",
    "dev:migrate": "ZESJE_SETTINGS=$(pwd)/zesje_dev_cfg.py FLASK_APP=zesje.wsgi:app flask db upgrade",
    "command:dev:prepare-migration": "ZESJE_SETTINGS=$(pwd)/zesje_dev_cfg.py FLASK_APP=zesje.wsgi:app flask db migrate",
    "dev:prepare-migration": "yarn dev:mysql is-running && (yarn command:dev:prepare-migration || true) || ((yarn dev:mysql start && sleep 2 && yarn command:dev:prepare-migration); yarn dev:mysql stop)",
    "test:py:cov": "yarn command:test:py --junitxml=tests.xml --cov=zesje --cov-report=xml:cov.xml --cov-report=html:cov.html --cov-report=term tests/",
    "dev:test:py:cov": "yarn dev:mysql start && yarn test:py:cov && yarn dev:mysql stop",
    "migrate-down": "FLASK_APP=zesje.wsgi:app flask db downgrade",
    "dev:mysql": "ZESJE_SETTINGS=$(pwd)/zesje_dev_cfg.py python3 -m zesje.mysql",
    "dev:mysql-init": "yarn dev:mysql create && sleep 2 && yarn dev:mysql-migrate",
    "dev:mysql-migrate": "yarn dev:mysql is-running && (yarn dev:migrate || true) || ((yarn dev:mysql start && sleep 2 && yarn dev:migrate); yarn dev:mysql stop)",
    "dev:oauth": "node $(pwd)/zesje/oauth2_mock_server.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__ZESJE_VERSION__",
      "alert",
      "confirm"
    ]
  },
  "dependencies": {
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bulma": "^0.7.1",
    "bulma-switch": "^2.0.0",
    "bulma-tooltip": "^2.0.1",
    "concurrently": "^3.6.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.6",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.2.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "humanize-duration": "^3.22.0",
    "image-dimensions-loader": "^0.1.0",
    "immutability-helper": "^2.7.0",
    "markdown-loader": "^3.0.0",
    "mathjs": "^6.6.4",
    "mini-css-extract-plugin": "^0.4.1",
    "mousetrap": "^1.6.2",
    "object-hash": "^2.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "path": "^0.12.7",
    "plotly.js-cartesian-dist": "^1.54.0",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-autosuggest": "^9.3.4",
    "react-bulma-components": "3.2.0",
    "react-bulma-notification": "^1.1.0",
    "react-bulma-switch": "^0.0.3",
    "react-dom": "^16.4.0",
    "react-dropzone": "^4.2.13",
    "react-hot-loader": "^4.3.3",
    "react-loadable": "^5.4.0",
    "react-pdf": "^3.0.5",
    "react-plotly.js": "^2.3.0",
    "react-rnd": "^8.0.2",
    "react-router-dom": "^4.3.1",
    "react-table": "^6.8.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.3.0",
    "jest-junit": "^10.0.0",
    "react-test-renderer": "^16.3.2",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^3.1.4"
  }
}
